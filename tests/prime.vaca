#(
    gcd <(a b -> (if (== 0 b) a (gcd b (mod a b))))
    coprimes <(a b -> (== 1 (gcd a b)))
    prime-iter <(i n -> (if (<= n (* i i)) 
                            true 
                            (if (coprimes n i) 
                                !(prime-iter (+ i 1) n)
                                false)))
    prime (prime-iter 1)
)

(assert (== (prime 1001723) true))